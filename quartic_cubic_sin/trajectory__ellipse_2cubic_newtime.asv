xm42=-a*cos(pi/(4/5*t00)*(t42-t30))+S1/2;
zm42=b*sin(pi/(4/5*t00)*(t42-t30))+Ze;
% 第二个三次曲线段
O2=[     0,         0,        0,         0,         S1,        0,            0,              0;
